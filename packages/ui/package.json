{
  "name": "@repo/ui",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "imports": {
    "#components/*": "./src/components/*",
    "#utils/*": "./src/utils/*"
  },
  "exports": {
    ".": "./dist/index.js",
    "./server-only": "./dist/server-only.js",
    "./cn": "./dist/utils/cn/index.js"
  },
  "scripts": {
    "build": "tsc -b",
    "dev": "tsc -w",
    "format": "prettier . --check --cache --cache-location='node_modules/.cache/prettiercache'",
    "format:fix": "prettier . --write --cache --cache-location='node_modules/.cache/prettiercache' --loglevel=warn",
    "lint": "eslint . --cache --cache-location 'node_modules/.cache/.eslintcache'",
    "lint:fix": "eslint . --fix --cache --cache-location 'node_modules/.cache/.eslintcache'",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-avatar": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-select": "^1.2.1",
    "@radix-ui/react-slot": "^1.0.1",
    "bright": "^0.8.4",
    "class-variance-authority": "^0.7.0",
    "clsx": "^1.2.1",
    "lucide-icons-react": "^1.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-wrap-balancer": "^1.1.0",
    "tailwind-merge": "^1.12.0"
  },
  "devDependencies": {
    "@repo/lint": "workspace:*",
    "@repo/tsconfig": "workspace:*",
    "@turbo/gen": "^1.9.8",
    "@types/react": "^18.2.21",
    "eslint": "^8.47.0",
    "typescript": "^5.1.6"
  }
}
