{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "dependsOn": ["@repo/prisma#db:generate"],
      "persistent": true,
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "dist/**"]
    },
    "topo": {
      "dependsOn": ["@repo/prisma#db:generate", "^topo"]
    },
    "//#format-root": {
      "dependsOn": ["^topo"]
    },
    "format": {
      "dependsOn": ["^topo"]
    },
    "lint": {
      "dependsOn": ["@repo/prisma#db:generate"],
      "outputs": ["node_modules/.cache/.eslintcache"]
    },
    "typecheck": {
      "dependsOn": ["@repo/prisma#db:generate"],
      "outputs": ["node_modules/.cache/tsbuildinfo.json"]
    }
  }
}
